<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.2.xsd">

	<bean id="address" class="com.fanlf.spring.beans.autowire.Address"
		p:city="BeiJing" p:street="HuiLongGuan"></bean>
		
	<bean id="address1" class="com.fanlf.spring.beans.autowire.Address"
		p:city="BeiJing" p:street="HuiLongGuan"></bean>		

	<bean id="car" class="com.fanlf.spring.beans.autowire.Car" p:brand="Audi"
		p:price="300000"></bean>

	<bean id="person" class="com.fanlf.spring.beans.autowire.Person" p:name="Tom"
		autowire="byName"></bean>

</beans>
